<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:pbeh="clr-namespace:Playnite.Behaviors;assembly=Playnite"
        xmlns:pin="clr-namespace:Playnite.Input;assembly=Playnite" xmlns:p="clr-namespace:Playnite;assembly=Playnite"
        mc:Ignorable="d">

    <Style TargetType="{x:Type Main}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Main}">
                    <Grid
                        x:Name="PART_MainHost"
                        d:DataContext="{x:Static DesignMainViewModel.DesignIntance}"
                        d:DesignHeight="{StaticResource DesignHeight}"
                        d:DesignWidth="{StaticResource DesignWidth}"
                        Background="Black">

                        <Image
                            x:Name="PS5DefaultImageNoVideo"
                            HorizontalAlignment="Left"
                            VerticalAlignment="Top"
                            RenderOptions.BitmapScalingMode="Fant"
                            Source="{ThemeFile 'Images/PS5/Background/PS5Background.png'}"
                            Stretch="Fill" />
                        <Grid x:Name="PART_ViewHost">

                            <Grid.Resources>
                                <p:BindingProxy x:Key="MAIN_Commands" Data="{Binding}" />
                                <p:BindingProxy x:Key="THEMEOPTIONS_Commands"
                                        Data="{Binding Content, ElementName=ThemeOptions_Command}" />
                            </Grid.Resources>

                            <Grid.InputBindings>
                                <pin:GameControllerInputBinding Button="Guide"
                                        Command="{Binding Content.GameControllerHoldCommand, ElementName=ThemeOptions_Command}">
                                    <pin:GameControllerInputBinding.CommandParameter>
                                        <x:Array Type="{x:Type Button}">
                                            <Button />
                                            <Button
                                                    Command="{Binding Data.ChangeProperty, Source={StaticResource THEMEOPTIONS_Commands}}"
                                                    CommandParameter="[Setter ElementName=PS5Menu, Property=IsChecked, Value=True]" />
                                        </x:Array>
                                    </pin:GameControllerInputBinding.CommandParameter>
                                </pin:GameControllerInputBinding>

                                <KeyBinding Key="C"
                                        Command="{Binding Content.ChangeProperty, ElementName=ThemeOptions_Command}"
                                        CommandParameter="[Setter ElementName=PS5Menu, Property=IsChecked, Value=True]" />
                            </Grid.InputBindings>

                            <Grid.Style>
                                <Style TargetType="Grid">
                                    <Setter Property="Opacity" Value="0" />
                                    <Style.Resources>
                                        <Storyboard x:Key="FlashStart">
                                            <DoubleAnimation BeginTime="0:0:0" Storyboard.TargetProperty="Opacity"
                                                    From="0" To="1" Duration="0:0:0.5" />
                                        </Storyboard>
                                    </Style.Resources>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding ElementName=WrongScreen, Path=Visibility}"
                                                Value="Visible">
                                            <Setter Property="Visibility" Value="Collapsed" />
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding ElementName=SetupScreen, Path=Visibility}"
                                                Value="Visible">
                                            <Setter Property="Visibility" Value="Collapsed" />
                                        </DataTrigger>
                                        <DataTrigger Binding="{Binding ElementName=LoginButton, Path=IsChecked}"
                                                Value="True">
                                            <DataTrigger.EnterActions>
                                                <BeginStoryboard Storyboard="{StaticResource FlashStart}" />
                                            </DataTrigger.EnterActions>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Style>
                            <CheckBoxEx x:Name="PS5Menu" IsChecked="False" Visibility="Collapsed">
                                <CheckBoxEx.Style>
                                    <Style TargetType="CheckBoxEx">
                                        <Style.Triggers />
                                    </Style>
                                </CheckBoxEx.Style>
                            </CheckBoxEx>
                            <Grid x:Name="PS5Background">
                                <!--MediaElement MediaElementBehaviors.Repeat="True" UnloadedBehavior="Stop" Source="{ThemeFile 'Videos/LoopNoParticles.mp4'}">
                                    <MediaElement.CacheMode>
                                        <BitmapCache RenderAtScale="1"/>
                                    </MediaElement.CacheMode>
                                    <MediaElement.Style>
                                        <Style TargetType="MediaElement">
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Window}}, Path=IsActive}" Value="False">
                                                    <Setter Property="MediaElement.Source" Value=" "/>
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding ElementName=PS5DefaultImage, Path=Opacity}" Value="1">
                                                    <Setter Property="MediaElement.Source" Value=" "/>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </MediaElement.Style>
                                </MediaElement-->
                                <Image x:Name="PS5DefaultImage" HorizontalAlignment="Left" VerticalAlignment="Top"
                                        RenderOptions.BitmapScalingMode="Fant"
                                        Source="{ThemeFile 'Images/PS5/Background/PS5Games.png'}" Stretch="Uniform">
                                    <Image.Style>
                                        <Style TargetType="Image">
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Settings Fullscreen.EnableMainBackgroundImage}"
                                                        Value="False">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                    <Setter Property="Opacity" Value="0" />
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding ElementName=LoginButton, Path=IsChecked}"
                                                        Value="True">
                                                    <DataTrigger.EnterActions>
                                                        <BeginStoryboard>
                                                            <Storyboard FillBehavior="Stop">
                                                                <DoubleAnimation BeginTime="0:0:0"
                                                                        Storyboard.TargetProperty="Opacity" From="0"
                                                                        To="0" Duration="0:0:2" />
                                                                <DoubleAnimation BeginTime="0:0:2"
                                                                        Storyboard.TargetProperty="Opacity" From="0"
                                                                        To="1" Duration="0:0:0.5" />
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </DataTrigger.EnterActions>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Image.Style>
                                </Image>
                            </Grid>

                            <!--  Game Background  -->

                            <Grid x:Name="GameBackground">
                                <FadeImage x:Name="PART_ImageBackground" SourceUpdateDelay="0"
                                        Tag="{DynamicResource ExplorePanel}">
                                    <FadeImage.Style>
                                        <Style TargetType="FadeImage">
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding ElementName=LoginButton, Path=IsChecked}"
                                                        Value="True">
                                                    <DataTrigger.EnterActions>
                                                        <BeginStoryboard>
                                                            <Storyboard FillBehavior="Stop">
                                                                <DoubleAnimation BeginTime="0:0:0"
                                                                        Storyboard.TargetProperty="Opacity" From="0"
                                                                        To="0" Duration="0:0:2" />
                                                                <DoubleAnimation BeginTime="0:0:2"
                                                                        Storyboard.TargetProperty="Opacity" From="0"
                                                                        To="1" Duration="0:0:0.5" />
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </DataTrigger.EnterActions>
                                                </DataTrigger>
                                                <MultiDataTrigger>
                                                    <MultiDataTrigger.Conditions>
                                                        <Condition
                                                                Binding="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                                                                Value="True" />
                                                        <Condition
                                                                Binding="{Binding ElementName=PS5StoreButtonDummy, Path=IsFocused}"
                                                                Value="True" />
                                                    </MultiDataTrigger.Conditions>
                                                    <MultiDataTrigger.Setters>
                                                        <Setter Property="Source" Value="" />
                                                        <Setter Property="Opacity" Value="0" />
                                                    </MultiDataTrigger.Setters>
                                                    <MultiDataTrigger.EnterActions>
                                                        <BeginStoryboard x:Name="FadeOutExploreTrue">
                                                            <Storyboard>
                                                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                                        To="0" Duration="0:0:0.2" />
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </MultiDataTrigger.EnterActions>
                                                    <MultiDataTrigger.ExitActions>
                                                        <RemoveStoryboard BeginStoryboardName="FadeOutExploreTrue" />
                                                    </MultiDataTrigger.ExitActions>
                                                </MultiDataTrigger>

                                                <MultiDataTrigger>
                                                    <MultiDataTrigger.Conditions>
                                                        <Condition
                                                                Binding="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                                                                Value="False" />
                                                        <Condition
                                                                Binding="{Binding ElementName=PS5StoreButtonDummy, Path=IsFocused}"
                                                                Value="True" />
                                                    </MultiDataTrigger.Conditions>
                                                    <MultiDataTrigger.Setters>
                                                        <Setter Property="Source" Value="" />
                                                    </MultiDataTrigger.Setters>
                                                    <MultiDataTrigger.EnterActions>
                                                        <BeginStoryboard x:Name="FadeOutExploreFalse">
                                                            <Storyboard>
                                                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                                        To="0" Duration="0:0:0.2" />
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </MultiDataTrigger.EnterActions>
                                                    <MultiDataTrigger.ExitActions>
                                                        <RemoveStoryboard BeginStoryboardName="FadeOutExploreFalse" />
                                                    </MultiDataTrigger.ExitActions>
                                                </MultiDataTrigger>
                                                <DataTrigger
                                                        Binding="{Binding ElementName=PS5ExploreButtonDummy, Path=IsFocused}"
                                                        Value="True">
                                                    <Setter Property="Source" Value="" />
                                                    <DataTrigger.EnterActions>
                                                        <BeginStoryboard x:Name="FadeOut2">
                                                            <Storyboard>
                                                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                                        To="0" Duration="0:0:0.2" />
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </DataTrigger.EnterActions>
                                                    <DataTrigger.ExitActions>
                                                        <RemoveStoryboard BeginStoryboardName="FadeOut2" />
                                                    </DataTrigger.ExitActions>
                                                </DataTrigger>
                                                <DataTrigger
                                                        Binding="{Binding ElementName=PS5MoreGamesButtonDummy, Path=IsFocused}"
                                                        Value="True">
                                                    <DataTrigger.EnterActions>
                                                        <BeginStoryboard x:Name="FadeOut3">
                                                            <Storyboard>
                                                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                                        To="0" Duration="0:0:0.2" />
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </DataTrigger.EnterActions>
                                                    <DataTrigger.ExitActions>
                                                        <RemoveStoryboard BeginStoryboardName="FadeOut3" />
                                                    </DataTrigger.ExitActions>
                                                </DataTrigger>
                                                <DataTrigger
                                                        Binding="{Binding ElementName=PS5PlusButtonDummy, Path=IsFocused}"
                                                        Value="True">
                                                    <DataTrigger.EnterActions>
                                                        <BeginStoryboard x:Name="FadeOut4">
                                                            <Storyboard>
                                                                <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                                        To="0" Duration="0:0:0.2" />
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </DataTrigger.EnterActions>
                                                    <DataTrigger.ExitActions>
                                                        <RemoveStoryboard BeginStoryboardName="FadeOut4" />
                                                    </DataTrigger.ExitActions>
                                                </DataTrigger>
                                                <Trigger Property="Opacity" Value="0">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </Trigger>
                                            </Style.Triggers>
                                        </Style>
                                    </FadeImage.Style>
                                </FadeImage>
                                <Image HorizontalAlignment="Left" VerticalAlignment="Top"
                                        RenderOptions.BitmapScalingMode="Fant"
                                        Source="{ThemeFile 'Images/PS5/Background/PS5Overlay.png'}" Stretch="Fill"
                                        Tag="{DynamicResource MicroTrailerBackground}">
                                    <Image.Style>
                                        <Style TargetType="Image">
                                            <Style.Triggers>
                                                <DataTrigger
                                                        Binding="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                                                        Value="True">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                                <DataTrigger Binding="{Settings Fullscreen.EnableMainBackgroundImage}"
                                                        Value="False">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                                <DataTrigger
                                                        Binding="{Settings Fullscreen.MainBackgroundImageDarkAmount}"
                                                        Value="0.0">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding ElementName=LoginButton, Path=IsChecked}"
                                                        Value="True">
                                                    <DataTrigger.EnterActions>
                                                        <BeginStoryboard>
                                                            <Storyboard FillBehavior="Stop">
                                                                <DoubleAnimation BeginTime="0:0:0"
                                                                        Storyboard.TargetProperty="Opacity" From="0"
                                                                        To="0" Duration="0:0:2" />
                                                                <DoubleAnimation BeginTime="0:0:2"
                                                                        Storyboard.TargetProperty="Opacity" From="0"
                                                                        To="1" Duration="0:0:0.5" />
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </DataTrigger.EnterActions>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Image.Style>
                                </Image>
                            </Grid>

                            <Grid x:Name="VideoBackgrounds" Tag="{DynamicResource MicroTrailerBackground}">
                                <Grid.Style>
                                    <Style TargetType="Grid">
                                        <Style.Triggers>
                                            <DataTrigger
                                                    Binding="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                                                    Value="false">
                                                <Setter Property="Visibility" Value="Collapsed" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Grid.Style>
                                <ContentControl x:Name="ExtraMetadataLoader_VideoLoaderControl_Main" HorizontalAlignment="Center" VerticalAlignment="Center" Focusable="False" />
                                <ContentControl x:Name="BackgroundChanger_PluginBackgroundImage" Visibility="Collapsed" Opacity="0"  Focusable="False" />
                                <MediaElement x:Name="VideoSourceBackgrounds" HorizontalAlignment="Center"
                                        MediaElementBehaviors.Repeat="True" Stretch="UniformToFill" IsMuted="True" Volume="0">

                                    <MediaElement.Style>
                                        <Style TargetType="MediaElement">
                                            <Setter Property="MediaElement.Source"
                                                    Value="{Binding ElementName=BackgroundChanger_PluginBackgroundImage, Path=Content.Source, NotifyOnTargetUpdated=True}" />
                                            <Setter Property="LoadedBehavior" Value="Close" />
                                            <Setter Property="UnloadedBehavior" Value="Stop" />
                                            <Setter Property="Opacity" Value="0" />
                                            <Setter Property="Visibility" Value="Collapsed" />
                                            <Style.Triggers>
                                                <EventTrigger RoutedEvent="Binding.TargetUpdated">
                                                    <BeginStoryboard>
                                                        <Storyboard FillBehavior="HoldEnd">
                                                            <DoubleAnimation Storyboard.TargetProperty="Opacity"
                                                                    From="0" To="0" Duration="0:0:001" />
                                                            <DoubleAnimation
                                                                    BeginTime="{StaticResource MicroTrailerTime}"
                                                                    Storyboard.TargetProperty="Opacity" From="0" To="1"
                                                                    Duration="0:0:0.5" />
                                                        </Storyboard>
                                                    </BeginStoryboard>
                                                </EventTrigger>
                                                <Trigger Property="Opacity" Value="0">
                                                    <Setter Property="LoadedBehavior" Value="Pause" />
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </Trigger>
                                                <MultiDataTrigger>
                                                    <MultiDataTrigger.Conditions>
                                                        <Condition
                                                                Binding="{Binding ElementName=LoginButton, Path=IsChecked}"
                                                                Value="True" />
                                                        <Condition
                                                                Binding="{PluginStatus Plugin=playnite-backgroundchanger-plugin, Status=Installed}"
                                                                Value="True" />
                                                        <Condition
                                                                Binding="{PluginSettings Plugin=BackgroundChanger, Path=EnableImageAnimatedBackground}"
                                                                Value="True" />
                                                        <Condition
                                                                Binding="{PluginSettings Plugin=BackgroundChanger, Path=BackgroundIsVideo}"
                                                                Value="True" />
                                                    </MultiDataTrigger.Conditions>
                                                    <MultiDataTrigger.Setters>
                                                        <Setter Property="LoadedBehavior" Value="Play" />
                                                        <Setter Property="Visibility" Value="Visible" />
                                                    </MultiDataTrigger.Setters>
                                                </MultiDataTrigger>
                                                <DataTrigger
                                                        Binding="{Binding ElementName=VideoSourceBackgrounds, Path=Visibility}"
                                                        Value="Collapsed">
                                                    <Setter Property="MediaElement.Source" Value=" " />
                                                </DataTrigger>
                                                <DataTrigger
                                                        Binding="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Window}}, Path=IsActive}"
                                                        Value="False">
                                                    <Setter Property="MediaElement.Source" Value=" " />
                                                </DataTrigger>
                                                <Trigger Property="Visibility" Value="Collapsed">
                                                    <Setter Property="Source" Value="" />
                                                </Trigger>
                                                <Trigger Property="Volume" Value="0.5">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </Trigger>
                                            </Style.Triggers>
                                        </Style>
                                    </MediaElement.Style>


                                </MediaElement>
                                <Image HorizontalAlignment="Left" VerticalAlignment="Top"
                                        RenderOptions.BitmapScalingMode="Fant"
                                        Source="{ThemeFile 'Images/PS5/Background/PS5Overlay.png'}" Stretch="Fill">
                                    <Image.Style>
                                        <Style TargetType="Image">
                                            <Style.Triggers>
                                                <DataTrigger
                                                        Binding="{Settings Fullscreen.MainBackgroundImageDarkAmount}"
                                                        Value="0.0">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                                <DataTrigger Binding="{Binding ElementName=LoginButton, Path=IsChecked}"
                                                        Value="True">
                                                    <DataTrigger.EnterActions>
                                                        <BeginStoryboard>
                                                            <Storyboard FillBehavior="Stop">
                                                                <DoubleAnimation BeginTime="0:0:0"
                                                                        Storyboard.TargetProperty="Opacity" From="0"
                                                                        To="0" Duration="0:0:2" />
                                                                <DoubleAnimation BeginTime="0:0:2"
                                                                        Storyboard.TargetProperty="Opacity" From="0"
                                                                        To="1" Duration="0:0:0.5" />
                                                            </Storyboard>
                                                        </BeginStoryboard>
                                                    </DataTrigger.EnterActions>
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Image.Style>
                                </Image>
                            </Grid>

                            <Grid x:Name="MoreGamesBackground">
                                <Grid.Style>
                                    <Style TargetType="Grid">
                                        <Setter Property="Visibility" Value="Collapsed" />
                                        <Style.Triggers>
                                            <DataTrigger
                                                    Binding="{Binding ElementName=PS5MoreGamesButtonDummy, Path=IsChecked}"
                                                    Value="True">
                                                <Setter Property="Visibility" Value="Visible" />
                                            </DataTrigger>
                                            <DataTrigger
                                                    Binding="{Binding ElementName=PART_ElemGameDetails, Path=Visibility}"
                                                    Value="Visible">
                                                <Setter Property="Visibility" Value="Collapsed" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Grid.Style>
                                <Image HorizontalAlignment="Left" VerticalAlignment="Top"
                                        RenderOptions.BitmapScalingMode="Fant"
                                        Source="{ThemeFile 'Images/PS5/Background/PS5Library.jpg'}" Stretch="Uniform" />
                            </Grid>

                            <Grid x:Name="WelcomeBackground">
                                <Grid.Style>
                                    <Style TargetType="Grid">
                                        <Setter Property="Visibility" Value="Collapsed" />
                                        <Style.Triggers>
                                            <DataTrigger
                                                    Binding="{Binding ElementName=PS5ExploreButtonDummy, Path=IsChecked}"
                                                    Value="True">
                                                <Setter Property="Visibility" Value="Visible" />
                                            </DataTrigger>
                                            <DataTrigger
                                                    Binding="{Binding ElementName=PART_ElemGameDetails, Path=Visibility}"
                                                    Value="Visible">
                                                <Setter Property="Visibility" Value="Collapsed" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Grid.Style>
                                <Image HorizontalAlignment="Left" VerticalAlignment="Top"
                                        RenderOptions.BitmapScalingMode="Fant"
                                        Source="{ThemeFile 'Custom/FakeWelcome/Backgrounds/ExploreBackground.jpg'}"
                                        Stretch="Fill">
                                    <Image.Style>
                                        <Style TargetType="Image">
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding ElementName=MediaLogic, Path=Text}"
                                                        Value="Media">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Image.Style>
                                </Image>
                                <Image HorizontalAlignment="Left" VerticalAlignment="Top"
                                        RenderOptions.BitmapScalingMode="Fant"
                                        Source="{ThemeFile 'Custom/FakeWelcome/Backgrounds/ExploreBackgroundMedia.jpg'}"
                                        Stretch="Fill">
                                    <Image.Style>
                                        <Style TargetType="Image">
                                            <Setter Property="Visibility" Value="Collapsed" />
                                            <Style.Triggers>
                                                <DataTrigger Binding="{Binding ElementName=MediaLogic, Path=Text}"
                                                        Value="Media">
                                                    <Setter Property="Visibility" Value="Visible" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Image.Style>
                                </Image>
                                <MediaElement
                                        Source="{ThemeFile 'Custom/FakeWelcome/Backgrounds/ExploreBackground.mp4'}"
                                        Tag="{DynamicResource ExploreVideo}" UnloadedBehavior="Close">
                                    <MediaElement.CacheMode>
                                        <BitmapCache RenderAtScale="1" />
                                    </MediaElement.CacheMode>
                                    <MediaElement.Style>
                                        <Style TargetType="MediaElement">
                                            <Setter Property="MediaElementBehaviors.Repeat" Value="True" />
                                            <Setter Property="LoadedBehavior" Value="Play" />
                                            <Setter Property="Visibility" Value="Collapsed" />
                                            <Style.Triggers>
                                                <DataTrigger
                                                        Binding="{Binding ElementName=WelcomeBackground, Path=Visibility}"
                                                        Value="Collapsed">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                                <MultiDataTrigger>
                                                    <MultiDataTrigger.Conditions>
                                                        <Condition
                                                                Binding="{Binding ElementName=WelcomeBackground, Path=Visibility}"
                                                                Value="Visible" />
                                                        <Condition
                                                                Binding="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                                                                Value="True" />
                                                    </MultiDataTrigger.Conditions>
                                                    <MultiDataTrigger.Setters>
                                                        <Setter Property="Visibility" Value="Visible" />
                                                    </MultiDataTrigger.Setters>
                                                </MultiDataTrigger>
                                                <MultiDataTrigger>
                                                    <MultiDataTrigger.Conditions>
                                                        <Condition
                                                                Binding="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                                                                Value="False" />
                                                    </MultiDataTrigger.Conditions>
                                                    <MultiDataTrigger.Setters>
                                                        <Setter Property="MediaElementBehaviors.Repeat" Value="False" />
                                                        <Setter Property="LoadedBehavior" Value="Stop" />
                                                    </MultiDataTrigger.Setters>
                                                </MultiDataTrigger>
                                                <DataTrigger
                                                        Binding="{Binding ElementName=WelcomeBackground, Path=Visibility}"
                                                        Value="Collapsed">
                                                    <Setter Property="MediaElement.Source" Value=" " />
                                                </DataTrigger>
                                                <DataTrigger
                                                        Binding="{Binding RelativeSource={RelativeSource FindAncestor, AncestorType={x:Type Window}}, Path=IsActive}"
                                                        Value="False">
                                                    <Setter Property="MediaElement.Source" Value=" " />
                                                    <Setter Property="LoadedBehavior" Value="Stop" />
                                                </DataTrigger>
                                                <Trigger Property="Visibility" Value="Collapsed">
                                                    <Setter Property="Source" Value="" />
                                                </Trigger>
                                                <DataTrigger Binding="{Binding ElementName=MediaLogic, Path=Text}"
                                                        Value="Media">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </MediaElement.Style>
                                </MediaElement>
                                <Border Background="Black" Opacity="{DynamicResource ExploreVideoOverlay}"
                                        Tag="{DynamicResource ExploreVideo}">
                                    <Border.Style>
                                        <Style TargetType="Border">
                                            <Setter Property="Visibility" Value="Collapsed" />
                                            <Style.Triggers>
                                                <DataTrigger
                                                        Binding="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                                                        Value="True">
                                                    <Setter Property="Visibility" Value="Visible" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Border.Style>
                                </Border>
                                <Image HorizontalAlignment="Left" VerticalAlignment="Top"
                                        RenderOptions.BitmapScalingMode="Fant"
                                        Source="{ThemeFile 'Images/PS5/Background/PS5Overlay.png'}" Stretch="Fill"
                                        Tag="{DynamicResource ExploreVideo}">
                                    <Image.Style>
                                        <Style TargetType="Image">
                                            <Style.Triggers>
                                                <DataTrigger
                                                        Binding="{Settings Fullscreen.MainBackgroundImageDarkAmount}"
                                                        Value="0.0">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                                <DataTrigger
                                                        Binding="{Binding RelativeSource={RelativeSource Self}, Path=Tag}"
                                                        Value="True">
                                                    <Setter Property="Visibility" Value="Collapsed" />
                                                </DataTrigger>
                                            </Style.Triggers>
                                        </Style>
                                    </Image.Style>
                                </Image>
                            </Grid>

                            <Grid x:Name="FakePlusBackgroundItems">
                                <Grid.Style>
                                    <Style TargetType="Grid">
                                        <Setter Property="Visibility" Value="Collapsed" />
                                        <Style.Triggers>
                                            <DataTrigger
                                                    Binding="{Binding ElementName=PS5PlusButtonDummy, Path=IsChecked}"
                                                    Value="True">
                                                <Setter Property="Visibility" Value="Visible" />
                                            </DataTrigger>
                                            <DataTrigger
                                                    Binding="{Binding ElementName=PART_ElemGameDetails, Path=Visibility}"
                                                    Value="Visible">
                                                <Setter Property="Visibility" Value="Collapsed" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Grid.Style>
                                <Grid x:Name="PlusBackgroundItem1">
                                    <Grid.Style>
                                        <Style TargetType="Grid">
                                            <Setter Property="Visibility" Value="Visible" />
                                        </Style>
                                    </Grid.Style>
                                    <Image HorizontalAlignment="Left" VerticalAlignment="Top"
                                            RenderOptions.BitmapScalingMode="Fant"
                                            Source="{ThemeFile 'Custom/FakePlus/Backgrounds/PlusItemBackground1.jpg'}"
                                            Stretch="Fill">
                                        <Image.Style>
                                            <Style TargetType="Image">
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=MediaLogic, Path=Text}"
                                                            Value="Media">
                                                        <Setter Property="Visibility" Value="Collapsed" />
                                                    </DataTrigger>
                                                </Style.Triggers>
